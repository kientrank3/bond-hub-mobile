      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.bondhub.cloud/api/v1"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.bondhub.cloud/api/v1"
      }
    }
  }
}
